        <rdfs:isDefinedBy rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/parameter.owl"/>
        <rdfs:label xml:lang="en">is parameter for</rdfs:label>
        <rdfs:label xml:lang="it">Ã¨ un parametro per</rdfs:label>
